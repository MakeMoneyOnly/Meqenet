{
  "type": "js",
  "includeCrypto": true,
  "includeFormulation": false,
  "noBanner": true,
  "skipInstall": true,
  "installDeps": false,
  "specVersion": "1.5",
  "noAtom": true,
  "noReachable": true,
  "noUsages": true,
  "exclude": [
    "**/node_modules/**",
    "**/build/**",
    "**/dist/**",
    "**/coverage/**",
    "**/logs/**",
    "**/tmp/**",
    "**/temp/**",
    "**/cache/**",
    "**/.cache/**",
    "**/.pnpm/**",
    "**/.nx/cache/**",
    "**/.next/**",
    "**/.git/**",
    "**/reports/**",
    "**/android/**",
    "**/ios/**",
    "**/*.gradle",
    "**/Podfile*",
    "**/gradlew*",
    "**/*.jar",
    "**/*.xcodeproj/**",
    "**/*.xcworkspace/**",
    "**/Bwoy/**",
    "**/terraform.zip",
    "**/prisma-engines.zip"
  ],
  "output": "reports/bom.json",
  "verbose": false,
  "deep": false,
  "evidence": false,
  "lifecycles": true
}