{
  "name": "@meqenet/{{cookiecutter.service_slug}}",
  "version": "{{cookiecutter.version}}",
  "description": "{{cookiecutter.service_description}}",
  "author": "{{cookiecutter.author_name}} <{{cookiecutter.author_email}}>",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "{{cookiecutter.node_version}}"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:integration": "jest --config ./test/jest-integration.json",{% if cookiecutter.needs_database == "y" %}
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",{% endif %}{% if cookiecutter.is_grpc_service == "y" %}
    "proto:generate": "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/generated --ts_proto_opt=nestJs=true --proto_path=proto proto/*.proto",{% endif %}
    "docker:build": "docker build -t {{cookiecutter.service_slug}} .",
    "docker:run": "docker run -p {{cookiecutter.service_port}}:{{cookiecutter.service_port}} {{cookiecutter.service_slug}}",
    "security:audit": "yarn audit --level moderate",
    "security:check": "snyk test"
  },
  "dependencies": {
    "@nestjs/common": "{{cookiecutter.nestjs_version}}",
    "@nestjs/core": "{{cookiecutter.nestjs_version}}",
    "@nestjs/platform-express": "{{cookiecutter.nestjs_version}}",
    "@nestjs/config": "^3.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/terminus": "^10.0.0",
    "@nestjs/throttler": "^5.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "bcrypt": "^5.1.0",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",{% if cookiecutter.needs_database == "y" %}
    "@prisma/client": "{{cookiecutter.prisma_version}}",{% endif %}{% if cookiecutter.is_grpc_service == "y" %}
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.8",
    "@nestjs/microservices": "{{cookiecutter.nestjs_version}}",{% endif %}{% if cookiecutter.is_event_driven == "y" %}
    "@nestjs/event-emitter": "^2.0.0",
    "amqplib": "^0.10.3",{% endif %}
    "prom-client": "^15.0.0",
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/sdk-node": "^0.45.0",
    "@opentelemetry/semantic-conventions": "^1.17.0",
    "@opentelemetry/exporter-jaeger": "^1.17.0",
    "@opentelemetry/instrumentation-http": "^0.44.0",
    "@opentelemetry/instrumentation-express": "^0.34.0",
    "winston": "^3.10.0",
    "nest-winston": "^1.9.4",
    "i18next": "^23.5.1",
    "nestjs-i18n": "^10.2.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "{{cookiecutter.nestjs_version}}",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@types/bcrypt": "^5.0.0",
    "@types/passport-jwt": "^3.0.9",{% if cookiecutter.needs_database == "y" %}
    "prisma": "{{cookiecutter.prisma_version}}",{% endif %}{% if cookiecutter.is_grpc_service == "y" %}
    "ts-proto": "^1.156.0",{% endif %}{% if cookiecutter.is_event_driven == "y" %}
    "@types/amqplib": "^0.10.1",{% endif %}
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "snyk": "^1.1238.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
} 