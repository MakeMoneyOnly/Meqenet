    "typescript": "~5.8.3",
    "vite": "7.0.3",
    "vitest": "^3.2.4",
    "webpack-cli": "^5.1.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
