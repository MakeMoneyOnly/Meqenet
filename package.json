{
  "name": "@meqenet/root",
  "version": "1.0.0",
  "description": "Meqenet.et - Ethiopia's Leading BNPL Financial Super-App",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "lint": "pnpm -r --if-present lint",
    "lint:fix": "eslint . --fix",
    "test": "jest --projects backend/services/auth-service backend/services/api-gateway backend-e2e",
    "build": "pnpm -r --if-present build",
    "security:audit": "pnpm audit --audit-level moderate",
    "security:secrets": "trufflehog git file://. --no-update --only-verified --fail",
    "security:outdated": "pnpm outdated",
    "validate:ethiopian-compliance": "echo 'Running Ethiopian FinTech compliance checks...' && pnpm run security:audit",
    "clean": "pnpm -r --if-present clean",
    "reset": "pnpm run clean && rm -rf node_modules pnpm-lock.yaml && pnpm install"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "@nestjs/platform-express": "^11.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@nx/eslint": "^21.2.1",
    "@nx/jest": "21.2.1",
    "@nx/js": "21.2.1",
    "@nx/nest": "^21.2.1",
    "@nx/node": "^21.2.1",
    "@nx/web": "21.2.1",
    "@nx/webpack": "21.2.1",
    "@nx/workspace": "^21.2.1",
    "@opentelemetry/api": "1.9.0",
    "@swc-node/register": "~1.10.10",
    "@swc/core": "~1.12.6",
    "@swc/helpers": "~0.5.17",
    "@types/jest": "^30.0.0",
    "@types/node": "~24.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "audit-ci": "^7.1.0",
    "axios": "^1.10.0",
    "eslint": "^9.29.0",
    "eslint-import-resolver-oxc": "^0.15.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jest": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.1.2",
    "npm-check-updates": "^18.0.1",
    "nx": "^21.2.1",
    "prettier": "^3.6.0",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "webpack-cli": "^6.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meqenet-et/meqenet.git"
  },
  "keywords": [
    "fintech",
    "bnpl",
    "ethiopia",
    "payments",
    "microservices",
    "telebirr",
    "fayda-id",
    "nbe-compliant"
  ],
  "author": "Meqenet.et Financial Services",
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "volta": {
    "node": "18.19.0",
    "pnpm": "8.15.1"
  },
  "packageManager": "pnpm@8.15.1",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "glob": ">=10.0.0",
        "inflight": ">=2.0.0"
      }
    },
    "overrides": {
      "glob@<10.0.0": "^10.4.5",
      "inflight@<2.0.0": "npm:@npmcli/fs@^3.1.0"
    },
    "packageExtensions": {
      "test-exclude@6.0.0": {
        "dependencies": {
          "glob": "^10.4.5"
        }
      }
    }
  }
}
