    "fayda-id",
    "nbe-compliant"
  ],
  "author": "Meqenet.et Financial Services",
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.0.0"
  },
  "volta": {
    "node": "22.17.0",
    "pnpm": "10.12.3"
  },
  "packageManager": "pnpm@10.12.3",
  "pnpm": {
    "onlyBuiltDependencies": [
