        "glob": ">=10.0.0",
        "inflight": ">=2.0.0",
        "chokidar": "3.x"
      }
    },
    "packageExtensions": {
      "test-exclude@6.0.0": {
        "dependencies": {
          "glob": "^10.4.5"
        }
      }
    }
  }
}
