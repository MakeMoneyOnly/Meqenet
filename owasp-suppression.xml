  <!-- ICU Library Vulnerabilities (React Native) -->
  <suppress>
    <notes>ICU Unicode libraries bundled with React Native 0.76.5 - Cannot be updated independently</notes>
    <packageUrl regex="true">.*icu.*</packageUrl>
    <cvssBelow>10.0</cvssBelow>
  </suppress>

